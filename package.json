{
  "name": "cypress-maildev",
  "version": "1.3.2",
  "description": "An easy-to-use cypress bunch of commands to use Maildev API for tests messages reception !",
  "main": "./build/index.js",
  "repository": "https://github.com/Clebiez/cypress-maildev.git",
  "author": "Cl√©ment Le Biez <clement.lebiez@gmail.com>",
  "license": "MIT",
  "scripts": {
    "wait-on": "npx wait-on http://localhost:1080",
    "test": "npx cypress open",
    "e2e": "npx cypress run",
    "fillEmail": "node ./fillEmail.js",
    "dev": "ts-node-dev --respawn  ./src/index.ts",
    "prod": "tsc --build && node ./build/index.js",
    "prepare": "npm run build",
    "build": "tsc"
  },
  "keywords": [
    "cypress",
    "e2e",
    "test",
    "maildev",
    "mail"
  ],
  "devDependencies": {
    "@types/cypress": "^1.1.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "cypress": "^12.2.0",
    "eslint": "^8.18.0",
    "nodemailer": "^6.7.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "wait-on": "^7.0.1"
  }
}
